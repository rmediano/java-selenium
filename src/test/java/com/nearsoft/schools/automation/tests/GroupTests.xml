<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="GroupTests" verbose="10" parallel="tests" thread-count="3">
    <test name="Smoke" preserve-order="true">
        <groups>
            <run>
                <include name="Smoke"/>
            </run>
        </groups>
        <classes>
            <class name="com.nearsoft.schools.automation.tests.FiltersTests"/>
            <class name="com.nearsoft.schools.automation.tests.LandingTest"/>
            <class name="com.nearsoft.schools.automation.tests.SearchTest"/>
        </classes>
    </test>
</suite>
